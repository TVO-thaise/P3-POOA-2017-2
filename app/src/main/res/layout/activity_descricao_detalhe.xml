    <Button
        android:id="@+id/bt_salvar_descricao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="25dp"
        android:text="Salvar"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/bt_alterar_descricao"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="210dp" />

    <Button
        android:id="@+id/bt_alterar_descricao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alterar"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="210dp"
        app:layout_constraintRight_toLeftOf="@+id/bt_deletar_descricao"
        android:layout_marginRight="8dp" />

    <Button
        android:id="@+id/bt_deletar_descricao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="33dp"
        android:text="Deletar"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="210dp" />

</android.support.constraint.ConstraintLayout>